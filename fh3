function OFFSH2()
  gg.setRanges(gg.REGION_ANONYMOUS)
gg.clearResults()
gg.searchNumber("0.38;2.5::6", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber("2.5", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(200)
gg.editAll("1", gg.TYPE_FLOAT)
gg.clearResults()
CD_toast('Отключено!')
end 

while(true) do
	MENUEND=0
	GEND=0

for i=1,999 do
	if MENUEND == 1 then GEND = 1 end
    if gg.isVisible(true) and MN == 4 and MENUEND == 0 then
    gg.setVisible(false)
    GEND=1
     end
 end

  if gg.isVisible(true) then
    gg.setVisible(false) 
        MENUEND=1
  end

    
   gg.setRanges(bit32.bxor(gg.REGION_ANONYMOUS,gg.REGION_C_ALLOC,gg.REGION_JAVA_HEAP,gg.REGION_C_HEAP,gg.REGION_C_DATA,gg.REGION_C_BSS,gg.REGION_BAD,gg.REGION_PPSSPP))
gg.skipRestoreState()

    if GEND==1 then MENUEND=0 and game() end
  if MENUEND==1 then MENU() end
end
