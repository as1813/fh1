function CD_searchNumber(n, type, ft, sign, r, s)
  local waring = "\n" .. "НЕ НАЖИМАЙ НА ИКОНКУ GG ВО ВРЕМЯ ПОИСКА, ИНАЧЕ ВЫЛЕТИТ СКРИПТ И ИГРА\n                                                        СДЕЛАНО ДЛЯ ЗАЩИТЫ ОТ КОПИРОВАНИЯ"
  gg.setVisible(false)
  gg.searchNumber(n, type, ft, sign, r, s)
  if gg.isVisible(true) then
    gg.setVisible(false)
    gg.processKill()
    print(waring)
    toast(waring)
    while true do
      gg.setVisible(false)
    end
  end
end



